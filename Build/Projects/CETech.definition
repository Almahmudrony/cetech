<?xml version="1.0" encoding="utf-8"?>
<Project Name="CETech" Path="sources/CETech" Type="Console">
  <References>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="SharpBgfx" />
    <Reference Include="MoonSharp" />
    <Reference Include="MsgPack" />
    <Reference Include="YamlDotNet" />
  </References>
  <Dependencies>
    <Uses Name="SDL2">
      <Platforms>Windows,MacOS,Linux</Platforms>
    </Uses>
    <Recommends Name="Develop">
      <Platforms>Windows,MacOS,Linux</Platforms>
    </Recommends>
  </Dependencies>
  <Services>
    <Service Name="SDL2">
      <AddDefine>CETECH_SDL2</AddDefine>
      <Reference Include="SDL2" />
      <Platforms>Windows,MacOS,Linux</Platforms>
    </Service>
    <Service Name="Develop">
      <AddDefine>CETECH_DEVELOP</AddDefine>
      <Reference Include="SQLite" />
      <Platforms>Windows,MacOS,Linux</Platforms>
    </Service>
  </Services>
  <Files>
    <Compile Include="Application\Application.cs" />
